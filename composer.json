{
    "name": "frogbob/laravel-blinky",
    "description": "Foundation Inky email templates in Laravel",
    "keywords": ["foundation", "inky", "laravel"],
    "license": "MIT",
    "authors": [
        {
            "name": "vanderb",
            "email": "freerk.vz@googlemail.com"
        }
    ],
    "require": {
        "frogbob/inky-php": "^0.0.1",
        "illuminate/support": "5.*|6.*",
        "illuminate/view": "5.*|6.*",
        "symfony/css-selector": "4.*|5.*",
        "symfony/dom-crawler": "4.*|5.*",
        "tijsverkoyen/css-to-inline-styles": "^2.2"
    },
    "autoload": {
        "psr-4": {

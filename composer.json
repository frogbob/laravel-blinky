{
    "name": "vanderb/laravel-blinky",
    "description": "Foundation Inky email templates in Laravel",
    "keywords": ["foundation", "inky", "laravel"],
    "license": "MIT",
    "authors": [
        {
            "name": "vanderb",
            "email": "freerk.vz@googlemail.com"
        }
    ],
    "require": {
        "hampe/inky": "^1.3.6.2",
        "illuminate/support": "5.*",
        "illuminate/view": "5.*",
        "symfony/css-selector": "4.*",
        "symfony/dom-crawler": "4.*",
        "tijsverkoyen/css-to-inline-styles": "^2.2"
    },
    "require-dev": {
        "graham-campbell/testbench": "^3.3",
        "phpunit/phpunit": "~5.4",
        "mockery/mockery": "^0.9.5"
    },
    "autoload": {
        "psr-4": {
            "Vanderb\\LaravelBlinky\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Vanderb\\Tests\\LaravelBlinky\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Vanderb\\LaravelBlinky\\BlinkyServiceProvider"
            ]
        }
    }
}
